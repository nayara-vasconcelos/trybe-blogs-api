{
  "name": "blogs-api",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "engineStrict": true,
  "private": true,
  "dependencies": {
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rescue": "^1.1.31",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "2.1.0",
    "sequelize": "6.3.4",
    "shelljs": "0.8.4"
  },
  "main": "src/server.js",
  "scripts": {
    "prestart": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "start": "node .",
    "drop": "npx sequelize-cli db:drop",
    "seed": "npx sequelize-cli db:seed:all",
    "dev": "nodemon .",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-trybe-backend": "1.0.3",
    "eslint-plugin-import": "2.22.1",
    "frisby": "2.1.2",
    "jest": "26.0.1",
    "jest-each": "^28.0.1",
    "nodemon": "^2.0.15",
    "sequelize-cli": "6.2.0",
    "sequelize-jest-helpers": "^1.0.2",
    "supertest": "4.0.2",
    "wait-on": "^6.0.1"
  }
}
